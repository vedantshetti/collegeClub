<%- layout('layouts/boilerplate') %>

<main class="container">
    <% if (userEmail === 'vedantshetti123456@gmail.com') { %>
        <!-- Users List -->
        <h2 class="section-title">Users</h2>
        <% if (users.length > 0) { %>
            <table>
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Email</th>
                        <th>Google ID</th>
                        <th>Username</th>
                    </tr>
                </thead>
                <tbody>
                    <% users.forEach(user => { %>
                        <tr>
                            <td><%= user._id %></td>
                            <td><%= user.email %></td>
                            <td><%= user.googleId || 'N/A' %></td>
                            <td><%= user.username || 'N/A' %></td>
                        </tr>
                    <% }); %>
                </tbody>
            </table>
        <% } else { %>
            <p class="no-users">No users found.</p>
        <% } %>

       
